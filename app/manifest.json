{
  "manifest_version": 1,
  "type": "WebApp",
  "$schema": "manifest-schema.json",
  "meta": {
    "version": "00.00.01",
    "minimum-overwolf-version": "0.190.0",
    "author": "Overwolf",
    "name": "Custom Overwolf Application",
    "description": "A General Purpose Overwolf app that implements React, TailwindCSS and Webpack",
    "dock_button_title": "Overwolf App",
    "icon": "icons/IconMouseOver.png",
    "icon_gray": "icons/IconMouseNormal.png",
    "launcher_icon": "icons/favicon.ico",
    "window_icon": "icons/WindowIcon.png"
  },
  "permissions": [
    "Hotkeys",
    "GameInfo",
    "GameControl",
    "Web",
    "Logging",
    "Extensions",
    "Profile",
    "Clipboard",
    "Media",
    "FileSystem",
    "Tray"
  ],
  "data": {
    "hotkeys": {
      "restart": {
        "title": "Restart (if bugged)",
        "hold": false,
        "action-type": "custom",
        "passthrough": false,
        "default": "Alt+Esc"
      }
    },
    "protocol_override_domains": {
      "localhost": "https",
      "doonline.nl": "https",
      "vercel.app": "https",
      "overwolf.com": "https"
    },
    "externally_connectable": {
      "matches": [
        "http://localhost:*",
        "http://doonline.nl:*",
        "http://*.vercel.app",
        "http://*.overwolf.com"
      ]
    },
    "force_browser": "user",
    "extra-objects": {},
    "start_window": "service",
    "windows": {
      "service": {
        "file": "service.html",
        "background_optimization": true,
        "is_background_page": true
      },
      "desktop": {
        "file": "desktop.html",
        "focus_game_takeover": "ShowOnHide",

        "desktop_only": true,

        "disable_rightclick": false,
        "ignore_keyboard_events": false,

        "show_in_taskbar": true,
        "disable_restore_animation": false,
        "grab_focus_on_desktop": true,
        "use_os_windowing": true,
        "native_window": true,

        "resizable": true,
        "transparent": false,
        "show_maximize": true,
        "show_minimize": true,

        "min_size": {
          "width": 1000,
          "height": 600
        },

        "allow_local_file_access": false,
        "keep_window_location": false,
        "override_on_update": true,
        "dev_tools_window_style": true,
        "optimize_accelerate_rendering": true
      },
      "overlay": {
        "file": "overlay.html",
        "focus_game_takeover": "ShowOnFocus",

        "in_game_only": true,

        "disable_rightclick": true,
        "ignore_keyboard_events": false,

        "show_in_taskbar": false,
        "disable_restore_animation": true,
        "grab_focus_on_desktop": false,
        "use_os_windowing": true,
        "native_window": true,

        "resizable": true,
        "transparent": true,
        "show_maximize": false,
        "show_minimize": false,

        "min_size": {
          "width": 100,
          "height": 0
        },

        "allow_local_file_access": false,
        "keep_window_location": false,
        "override_on_update": true,
        "dev_tools_window_style": true,
        "optimize_accelerate_rendering": true
      }
    },
    "game_targeting": {
      "type": "dedicated",
      "game_ids": [
        21816
      ]
    },
    "game_events": [
      21816
    ],
    "launch_events": [
      {
        "event": "GameLaunch",
        "event_data": {
          "game_ids": [
            21816
          ]
        },
        "start_minimized": false
      }
    ],
    "launch_event": [
      {
        "event": "GameLaunch",
        "game_ids": [
          21816
        ],
        "start_minimized": false
      }
    ],
    "developer": {
      "enable_auto_refresh": true,
      "reload_delay": 1000,
      "filter": "*.*"
    }
  }
}